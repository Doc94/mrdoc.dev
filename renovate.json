{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchPackageNames": ["*"],
      "rangeStrategy": "bump"
    },
    {
      "matchPackagePatterns": ["^@iconify-json\\/"],
      "groupName": "Iconify JSON packages"
    }
  ]
}
